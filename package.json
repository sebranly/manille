  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sebranly/manille/issues"
  },
  "homepage": "https://github.com/sebranly/manille#readme",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/lodash": "^4.14.182",
    "eslint": "^8.20.0",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  }
