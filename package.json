{
  "name": "manille",
  "version": "0.0.5",
  "files": [
    "lib/**/*"
  ],
  "description": "Manille Engine",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
