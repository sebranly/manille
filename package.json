  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "eslint": "^7.20.0",
    "jest": "^26.6.3",
    "npx": "^10.2.2",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.1",
